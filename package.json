{
  "name": "addargs",
  "version": "0.0.1",
  "description": "Add auto-ignored arguments to a function so that an external API accepts it as a callback",
  "main": "./distribution/addArgs.js",
  "scripts": {
    "lint": "eslint src tools",
    "test": "mocha \"src/**/*.test.js\" --compilers js:babel-register",
    "clean": "rm -rf ./dist",
    "build": "babel src --presets babel-preset-es2015 --out-dir dist",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cfogelberg/addargs.git"
  },
  "keywords": [
    "callbacks",
    "javascript"
  ],
  "author": "Christo Fogelberg <cfogelberg@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cfogelberg/addargs/issues"
  },
  "homepage": "https://github.com/cfogelberg/addargs#readme",
  "devDependencies": {
    "babel-cli": "6.18.0",
    "babel-eslint": "7.1.1",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-stage-0": "6.16.0",
    "chai": "3.5.0",
    "eslint": "3.13.1",
    "eslint-loader": "1.6.1",
    "eslint-plugin-babel": "4.0.1",
    "mocha": "3.2.0",
    "sinon": "2.0.0-pre.5"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "airbnb",
    "env": {
      "browser": true
    },
    "rules": {
      "no-confusing-arrow": 0
    }
  }
}
